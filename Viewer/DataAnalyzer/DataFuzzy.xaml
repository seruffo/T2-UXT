<Window x:Class="Lades.WebTracer.DataFuzzy"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lades.WebTracer"
        mc:Ignorable="d"
        Title="DataFuzzy" Height="300" Width="300" ContentRendered="Window_ContentRendered" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid Background="Black">
        <TextBox x:Name="txt_analysis" Text="Loading...&#xA;" FontSize="14" Margin="0,0,0,33" AcceptsReturn="True" Foreground="#ff111111"  HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>
        <Button Content="Salvar" HorizontalAlignment="Right" Margin="0,0,7,7" VerticalAlignment="Bottom" Width="75" Click="Button_Click"/>
        <Label Content="WAVES Systems" HorizontalAlignment="Left" Margin="1,0,0,4" VerticalAlignment="Bottom" Foreground="#FFACACAC"/>

    </Grid>
</Window>
